{
  "name": "eaxcess-bot",
  "version": "1.0.0",
  "description": "Simple Boilerplate code for a webpack-sass-typescript application",
  "private": true,
  "main": "dist/bot.bundle.js",
  "scripts": {
    "devOld": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack serve --config webpack.dev.js",
    "start": "npm run dev",
    "build:dev": "webpack --watch --config webpack.dev.js",
    "serve:dev": "nodemon",
    "dev": "concurrently \"npm run build:dev\"  \"npm run serve:dev\" "
  },
  "keywords": [
    "js",
    "telegram"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/typescript": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.3",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "telegraf": "^3.38.0"
  }
}
