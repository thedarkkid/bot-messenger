{
  "name": "bot-messenger",
  "version": "0.1.0",
  "description": "Simple scaffolding to make the creation of telegram bots easier.",
  "private": true,
  "main": "dist/bot.bundle.js",
  "scripts": {
    "devOld": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack serve --config webpack.dev.js",
    "start": "npm run dev",
    "build:dev": "webpack --watch --config webpack.dev.js",
    "serve:dev": "nodemon",
    "dev": "concurrently \"npm run build:dev\"  \"npm run serve:dev\" "
  },
  "keywords": [
    "js",
    "telegram"
  ],
